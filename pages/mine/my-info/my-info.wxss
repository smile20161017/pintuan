.title{
    box-sizing: border-box;
    width: 100%;
    height: 90rpx;
    padding: 0 24rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #666666;
    line-height: 90rpx;
}
.list{
    width: 100%;
    height: auto;
    border-radius: 12rpx;
    background: white;
}
.box{
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: calc(100% - 48rpx);
    height: 100rpx;
    padding: 0 24rpx 0 24rpx;
    font-size: 28rpx;
    font-weight: 400;
    border-bottom: 1rpx solid #EEEEEE;
}
.list .box:nth-of-type(1){
  	height: 120rpx;
}
.box-last{
    border-bottom: none;
}
.box-left{
    color: #333333;
}
.box-right{
    display: flex;
    align-items: center;
    color: #666666;
}
.box-right image{
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
}
.box-right .iconfont{
    margin-left: 18rpx;
    color: #666666;
}
.flex-box{
    display: flex;
    align-items: center;
}