page{
    background: #f5f6f7;
}
.title{
    display: flex;
    flex-flow: nowrap;
    justify-content: space-between;
    align-items: center;
    width: calc(100% - 48rpx);
    height: 90rpx;
    padding: 0 24rpx 0 24rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #333333;
}
.title view .iconfont{
    margin-right: 15rpx;
}
.title .iconweixin{
    padding-top: 10rpx;
    font-size: 36rpx;
    color: #129611;
}
.iconxiangyou{
    color: #666666;
}
#add-form{
    width: 100%;
    height: auto;
    border-radius: 12rpx;
    background: white;
}
.box{
    box-sizing: border-box;
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    width: 100%;
    height: 100rpx;
    padding: 0 24rpx;
    border-bottom: 1rpx solid #DDDDDD;
}
.box .weui-cell__hd{
    width: 160rpx;
    color: #333333;
    font-size: 28rpx;
    font-weight: 400;
}
.box-last{
    border-bottom: none;
}
.box-last .weui-cell__hd{
    width: 200rpx;
}
.box .weui-cell__bd{
    flex: 1;
    font-size: 28rpx;
    font-weight: 400;
    color: #999999;
}
.box .weui-cell__bd input{
    font-size: 28rpx;
    font-family: "Source Han Sans CN";
    font-weight: 400;
    color: #999999;
}
.weui-cell:before{
    border-top: none !important;
}
.picker{
    font-size: 28rpx;
    font-weight: 400;
    color: #333333;
}
.text-gray{
    color: #999999;
}

.btn{
    width: calc(100% - 48rpx);
    height: 90rpx;
    margin: 0 24rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 34rpx;
    font-weight: 500;
    border-radius: 45rpx;
}
.btn-save{
    margin-top: 53rpx;
    color: white;
    background: linear-gradient(-90deg, rgba(254,38,0,1) 0%, rgba(239,36,0,1) 100%);
}
.btn-del{
    margin-top: 35rpx;
    color: #333333;
    background: linear-gradient(-90deg,rgba(255,255,255,1) 0%,rgba(255,255,255,1) 100%);
    border: 1px solid rgba(214, 214, 214, 1);
}
.region-box{
    display: flex;
    justify-content: space-between;
    align-items: center;
}