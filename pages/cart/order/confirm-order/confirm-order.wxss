page {
	font-size: 26rpx;
	color: #333333;
	background-color: #F5F6F7;
}
.address-infor {
	display: flex;
	align-items: center;
	position: relative;
	height: 188rpx;
	padding: 0 116rpx;
	background-color: #FFFFFF;
	border-top: 20rpx solid #F5F6F7;
	border-bottom: 20rpx solid #F5F6F7;
	box-sizing: border-box;
}
.address-infor .icondaohangdizhi {
	position: absolute;
	top: 50%;
	left: 24rpx;
	width: 68rpx;
	height: 68rpx;
	line-height: 68rpx;
	text-align: center;
	color: #FFFFFF;
	background-color: #EF2400;
	border-radius: 50%;
	transform: translateY(-50%);
}
.address-infor .iconxiangyou {
	position: absolute;
	top: 50%;
	right: 24rpx;
	color: #C9C9C9;
	transform: translateY(-50%);
}
.address-infor .name {
	font-size: 28rpx;
}
.address-infor .tel {
	margin-left: 20rpx;
	color: #999999;
}
.address-infor .address {
	margin-top: 10rpx;
	line-height: 32rpx;
}
.detail {
	padding-bottom: 150rpx;
	background-color: #FFFFFF;
	box-sizing: border-box;
}
.list-item {
	padding: 40rpx 24rpx 0;
	border-bottom: 20rpx solid #F5F6F7;
}
.list-item .item {
	padding: 40rpx 0;
}
.product, .pres {
	display: flex;
	align-items: center;
	justify-content: space-between;
	position: relative;
	box-sizing: border-box;
}
.product {
	margin-bottom: 20rpx;
}
.product-img {
	display: flex;
	align-items: center;
	justify-content: center;
	width: 180rpx;
	height: 180rpx;
	border: 1rpx solid #DDDDDD;
	border-radius: 8rpx;
}
.product-img.lens-img {
	height: 90rpx;
	overflow: hidden;
}
.product-img image {
	display: block;
	width: 95%;
}
.product-des {
	width: 370rpx;
	min-height: 100%;
}
.product-name {
	display: block;
	margin-bottom: 15rpx;
	line-height: 36rpx;
}
.product-pres {
	display: block;
	padding: 5rpx 15rpx;
	line-height: 42rpx;
	color: #999999;
	background-color: #F1F1F1;
	border-radius: 8rpx;
	box-sizing: border-box;
}
.product-price {
	width: 100rpx;
	text-align: right;
}
.item {
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 30rpx 24rpx;
	font-size: 28rpx;
	background-color: #FFFFFF;
}
.item.total-num {
	padding-top: 40rpx;
}
.item .item-left {
	width: 180rpx;
	text-align: right;
}
.item .item-right.choose-infor {
	display: flex;
	align-items: center;
	justify-content: space-between;
	width: 495rpx;
}
.item .item-right.choose-infor .iconxiangyou {
	margin-left: 10rpx;
	font-size: 24rpx;
	color: #C9C9C9;
}
.way {
	color: #999999;
}
.choose-coupon {
	color: #EF2400;
}
.choose-coupon.selected {
	color: #999999;
}
.coupon-num {
	display: flex;
	align-items: center;
}
.total {
	display: flex;
	align-items: center;
	justify-content: flex-end;
	padding: 30rpx 24rpx 0;
	font-size: 28rpx;
}
.total .num {
	margin-right: 10rpx;
	font-size: 26rpx;
	color: #999999;
}
.total-price {
	color: #EF2400;
}
.btn-list {
	position: fixed;
	left: 0;
	bottom: 0;
	display: flex;
	align-items: center;
	justify-content: flex-end;
	width: 100%;
	height: 110rpx;
	padding: 0 24rpx;
	background-color: #FFFFFF;
	border-top: 1rpx solid #DDDDDD;
	box-sizing: border-box;
	z-index: 100;
}
.btn-list .settle {
	width: 280rpx;
	height: 90rpx;
	margin-left: 30rpx;
	text-align: center;
	line-height: 90rpx;
	font-size: 34rpx;
	color: #FFFFFF;
	background: #EF2400;
	border-radius: 45rpx;
}
.btn-list .btn-total {
	font-size: 32rpx;
}
.btn-list .btn-total text {
	font-size: 26rpx;
	color: #EF2400;
}
.btn-list .btn-total text:nth-child(2) {
	font-size: 34rpx;
}
.coupon-wrap .content-item {
	position: relative;
	display: flex;
	align-items: center;
	width: 702rpx;
	height: 140rpx;
	margin-bottom: 30rpx;
	box-sizing: border-box;
	color: #EF2400;
	border-radius: 8rpx;
}
.coupon-wrap .content-item image {
	position: absolute;
	top: 0;
	left: 0;
	width: 702rpx;
	height: 140rpx;
	z-index: -1;
}
.coupon-wrap .coupon-infor {
	display: flex;
	align-items: center;
	width: 75%;
}
.coupon-wrap .price {
	display: flex;
	align-items: baseline;
	width: 170rpx;
	padding-left: 40rpx;
	box-sizing: border-box;
}
.coupon-wrap .price.cancel {
	width: 300rpx;
}
.coupon-wrap .content-item .price text:nth-child(1) {
	font-size: 30rpx;
}
.coupon-wrap .content-item .price text:nth-child(2) {
	font-size: 56rpx;
}
.coupon-wrap .content-item .msg {
	display: flex;
	flex-direction: column;
	align-items: flex-start;
	margin: 0 20rpx 0 25rpx;
}
.coupon-wrap .content-item .msg text:nth-child(1) {
	font-size: 26rpx;
	margin-bottom: 5rpx;
}
.coupon-wrap .content-item .msg text:nth-child(2) {
	font-size: 20rpx;
}
.coupon-wrap .content-item .select {
	display: flex;
	align-items: center;
	justify-content: center;
	width: 25%;
	height: 104rpx;
	border-left: 2rpx dashed #EF2400;
}
.coupon-wrap .content-item .circle {
	width: 20px;
	height: 20px;
	border-radius: 50%;
	background-color: #FFFFFF;
	border: 1rpx solid #DDDDDD;
	box-sizing: border-box;
} 
