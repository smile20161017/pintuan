/**app.wxss**/
@import 'utils/css/iconfont.wxss';

.container {
	display: flex;
	flex-direction: column;
	align-items: center;
	height: 100%;
	background-color: #F5F6F7;
}
image {
	height: auto;
}

.none-wrap{
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	width: 100%;
	padding-top: 128rpx;
}
.none-wrap image{
	width: 196rpx;
	height: 168rpx;
}
.none-wrap text:nth-of-type(1){
	margin-top: 85rpx;
	line-height: 52rpx;
	font-size: 34rpx;
	font-weight: 400;
	color: #333333;
}
.none-wrap text:nth-of-type(2){
	line-height: 52rpx;
	font-size: 28rpx;
	font-weight: 400;
	color: #999999;
}